{
  "name": "bondage-club-headless-template",
  "version": "0.4.0",
  "description": "Template for custom scripts for headless client for Bondage Club",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "start": "yarn run build && node --trace-warnings --unhandled-rejections=strict build/index.js",
    "start-dev": "yarn run build && node --trace-warnings --unhandled-rejections=strict --inspect build/index.js",
    "test": "tsc --noEmit -p . && eslint src"
  },
  "author": "Jomshir98",
  "license": "MIT",
  "engines": {
    "node": ">= 12.4.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.2",
    "bondage-club-bot-api": "./bondage-club-bot-api-v0.4.0.tgz",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
